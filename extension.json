{
	"name": "GoogleTranslator",
	"version": "0.4.",
	"author": "Zoran Dori with customization",
	"url": "https://www.mediawiki.org/wiki/Extension:GoogleTranslator",
	"descriptionmsg": "googletranslator-desc",
	"license-name": "MIT",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.39.0"
	},
	"ExtensionMessagesFiles": {
		"GoogleTranslatorAlias": "GoogleTranslator.alias.php"
	},
	"AutoloadClasses": {
		"GoogleTranslator": "GoogleTranslator.php"
	},
	"Hooks": {
		"SkinBuildSidebar": "GoogleTranslator::forGoogleTranslatorInSidebar",
		"SkinAfterContent": "GoogleTranslator::forGoogleTranslatorAfterContent"
	},
	"config": {
		"GoogleTranslatorOriginal": {
			"value": "en"
		},
		"GoogleTranslatorLanguages": {
			"value": "de,fr,sr"
		}
	},
	"manifest_version": 2
}
